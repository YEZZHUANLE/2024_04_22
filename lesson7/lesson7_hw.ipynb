   "source": [
    "import pyinputplus as pyip\n",
    "factors=pyip.inputInt(\"請輸入整數,求所有因數:\");\n",
    "print(f\"輸入整數為{factors}\")\n",
    "print(f\"{factors}的因數是\")\n",
    "\n",
    "for i in range(1, factors+1):\n",
    "    if factors % i == 0:\n",
    "        print(i,end=' ')\n",
    "        \n",
    "\n"
   ]
  }
